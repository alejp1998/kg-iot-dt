    "namespace": {
        "eri": "https://ericsson.com/namespace"
    },
    "defaultNamespace": "eri",
    "sdfObject": {
        "rain_sensor": {
            "description": "Measures the amount of rain that is falling.",
            "sdfProperty": {
                "uid": {
                    "description": "Unique module identifier.",
                    "type": "string"
                },
                "cumdepth": {
                    "description": "Cumulative rain fall depth.",
                    "type": "number",
                    "unit": "mm"
                }
            }
        },
        "time_control": {
            "description": "Provides data about the device timing.",
            "sdfProperty": {
                "uid": {
                    "description": "Unique module identifier.",
                    "type": "string"
                },
                "timestamp": {
                    "description": "Moment when the last device report was sent.",
                    "type": "string",
                    "format": "date-time"
                }
            }   
        }
    }
}
        "license": "https://ericsson.com/license"
    },
    "namespace": {
      "eri": "https://ericsson.com/namespace"
    },
    "defaultNamespace": "eri",
    "sdfObject": {
        "repair_control": {
            "description": "Notifies the production line of the status of a product repair.",
            "sdfProperty": {
                "uid": {
                    "description": "Unique module identifier.",
                    "type": "string"
                },
                "repairstatus": {
                    "description": "Repair Status - Done/Ongoing/Idle",
                    "enum": ["done","ongoing","idle"],
                    "type": "string"
                }
            }   
        },
        "time_control": {
            "description": "Provides data about the device timing.",
            "sdfProperty": {
                "uid": {
                    "description": "Unique module identifier.",
                    "type": "string"
                },
                "timestamp": {
                    "description": "Moment when the last device report was sent.",
                    "type": "datetime"
                }
            }   
        }
    }
}